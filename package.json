{
  "name": "@outdoorsyco/ember-cli-crowdin",
  "version": "3.2.6-beta.1",
  "description": "Manages the Crowdin translations for your Ember app from ember-cli",
  "keywords": [
    "ember-addon",
    "i18n",
    "internationalization",
    "translation",
    "crowdin",
    "ember",
    "ember-i18n"
  ],
  "license": "MIT",
  "author": "Outdoorsy",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/outdoorsy/ember-cli-crowdin",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test",
    "deploy": "npm run version:patch && npm run push:git",
    "deploy:ci": "npm run version:patchCI && npm run push:git",
    "version:patch": "npm version patch",
    "version:patchCI": "npm version patch -m 'CI deploy version %s so [skip ci]'",
    "push:git": "git push --tags origin HEAD:master"
  },
  "dependencies": {
    "@outdoorsyco/crowdin-api": "2.1.4",
    "crowdin-node": "github:outdoorsy/crowdin-node#updates",
    "ember-cli-babel": "^6.12.0",
    "minimatch": "^3.0.4",
    "unzipper": "^0.8.14"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.7.0",
    "broccoli-stew": "^2.0.1",
    "chalk": "^1.1.1",
    "colors": "^1.3.3",
    "diff": "^3.5.0",
    "ember-ajax": "^3.1.0",
    "ember-cli": "^3.1.4",
    "ember-cli-dependency-checker": "^2.1.1",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.5.5",
    "ember-source": "^2.18.2",
    "ember-try": "1.1.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^5.2.1",
    "js-yaml": "^3.12.0",
    "loader.js": "^4.7.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": "6.* || >= 7.* || >= 8.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
